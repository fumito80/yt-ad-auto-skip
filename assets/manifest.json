{
  "version": "0.6.2",
  "name": "YT Ad-min",
  "description": "Automate mute, fast-forward, and click the \"Skip\" button on YouTube ads.",
  "manifest_version": 3,
  "icons": {
    "128": "promo128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "https://www.youtube.com/*"
  ]
}